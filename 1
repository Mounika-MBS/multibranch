https://github.com/Mounika-MBS/multibranch.git
pipeline {
        agent any
	stages { 
	    stage('HOTFIX branch') {
	    steps {
	      sh 'echo "this is hotfix branch"'
	      }
	      }
	      stage ('sprint1') {
	      steps { 
	      sh 'echo "sprint1 application"'
	      }
	      }

	      stage("deploy app") {
	      steps{
	      sh 'echo "deploying application"'
	      }
	      }
	      }
	      }

